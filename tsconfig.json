{
	"compilerOptions": {
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"@angular/*": [
				"./node_modules/@angular/*"
			],
			"_shared": [
				"./src/app/shared"
			],
			"_shared/*": [
				"./src/app/shared/*"
			],
			"_states": [
				"./src/app/states"
			],
			"_states/*": [
				"./src/app/states/*"
			]
		},
		"module": "es2015",
		"target": "es5",
		"noImplicitAny": false,
		"inlineSourceMap": true,
		"sourceMap": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"lib": [
			"es2016",
			"dom"
		],
		"typeRoots": [
			"./node_modules/@types"
		],
	},
	"angularCompilerOptions": {
		"entryModule": "./app/app.module#AppModule",
		"genDir": "./dist",
		"debug": true
	},
	"types": [
		"types/node",
		"source-map",
		"types",
		"express"
	],
	"include": [
		"src"
	]
}
